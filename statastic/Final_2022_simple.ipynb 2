     ]
    }
   ],
   "source": [
    "# studentized residual\n",
    "print(\"Standardized Residuals \\n\")\n",
    "st1, data1, ss3 = sso.summary_table(fb_result, alpha=0.05)\n",
    "SD = data1[:, 10]\n",
    "mu = np.mean(SD)\n",
    "sigma = np.std(SD)\n",
    "print(SD)\n",
    "print('mu = ', mu)\n",
    "print('sigma = ', sigma)\n"
   ]
